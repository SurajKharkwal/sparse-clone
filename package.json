{
  "name": "@flyinghawk/sparse-clone",
  "version": "0.0.3",
  "description": "A lightweight and efficient clone utility package",
  "keywords": [
    "sparse-clone",
    "clone",
    "utility",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SurajKharkwal/sparse-clone"
  },
  "author": "Suraj Kharkwal",
  "license": "MIT",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "index.ts"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
